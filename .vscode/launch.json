    {
      "type": "node",
      "request": "launch",
      "name": "Serve",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/@explorablegraph/eg/src/eg.js",
      "args": ["serve(:${file})"],
      "runtimeExecutable": "/Users/jan/.nvm/versions/node/v16.0.0/bin/node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Spike",
