  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "eg",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/@explorablegraph/eg/src/eg.js",
      "cwd": "${workspaceFolder}/src/stage",
      "args": []
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Serve",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/@explorablegraph/eg/src/eg.js",
      "args": ["serve(:${file})"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Spike",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}"
    }
  ]
}
